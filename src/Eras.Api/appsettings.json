{
    "CosmicLatte": {
      "BaseUrl": "${COSMIC_LATTE_BASE_URL}",
      "ApiKey": "${COSMIC_LATTE_API_KEY}"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ConnectionStrings": {
        "ErasConnection": "Host=${DB_HOST};Port=${DB_PORT};Username=${DB_USER};Password=${DB_PASSWORD};Database=${DB_NAME}"
    },
    "AllowedHosts": "*",
    "Keycloak": {
        "BaseUrl": "${KEYCLOAK_BASE_URL}",
        "Realm": "${KEYCLOAK_REALM}",
        "ClientId": "${KEYCLOAK_CLIENT_ID}",
        "ClientSecret": "${KEYCLOAK_CLIENT_SECRET}"
    }
}