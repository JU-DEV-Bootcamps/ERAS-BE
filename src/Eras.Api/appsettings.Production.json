{
  "CosmicLatte": {
    "BaseUrl": "__COSMIC_API_URL__",
    "ApiKey": "__COSMIC_LATTE_API_KEY__"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "ErasConnection": "Host=__DB_HOST__;Port=5432;Username=eras_user;Password=eras_password;Database=eras_db;SslMode=Require;TrustServerCertificate=True"
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:5443",
        "Certificate": {
          "Path": "/https/aspnetapp.pfx",
          "Password": "eras"
        }
      }
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "BaseUrl": "__KEYCLOAK_URL__",
    "Realm": "ERAS",
    "ClientId": "api-client",
    "ClientSecret": "__KEYCLOAK_CLIENT_SECRET__"
  }
}
